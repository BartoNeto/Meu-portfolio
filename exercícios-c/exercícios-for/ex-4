#include<stdio.h>

int main()
{
   int I,N,P=0,IM=0;            
for(I=1;I<=10;I++){
printf("Digite um número:");
scanf("%i",&N);
if(N>=10&&N<=20)
P++;
else
IM++;
}
printf("A quantidade de números entre 10 e 20 é %i\n",P);
printf("A quantidade de números que não estão entre 10 e 20 é %i",IM);
    return 0;
}
